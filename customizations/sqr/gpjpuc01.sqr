! Print_Reason_for_Unemp                                               *
! Called By:    Print-Report                                           *
! Description:  Prints the Reason for Unemployment.                    *
!***********************************************************************

begin-procedure Print_Reason_for_Unemp

EVALUATE &HE.GPJP_UC_TERM_RSN
        WHEN = '001'
                PRINT 'Åõ'                      (#SE701,#LI7)
                BREAK
        WHEN = '002'
                PRINT 'Åõ'                      (#SE702,#LI7)
                BREAK
        WHEN = '003'
                PRINT 'Åõ'                      (#SE703,#LI7)
                PRINT &HE.GPJP_UC_RETIRE_AGE    (#SE703,#LI7TE)
                BREAK
        WHEN = '004'
                PRINT 'Åõ'                      (#SE704,#LI7)
                BREAK
        WHEN = '005'
                PRINT 'Åõ'                      (#SE705,#LI7)
                PRINT &HE.GPJP_UC_MTH_CONTR     (#SE7A,#LI7_1)
                PRINT &HE.GPJP_UC_TOT_MTH       (#SE7A,#LI7TU)
                PRINT &HE.GPJP_UC_NUM_UPDT      (#SE7A,#LI7KE)
                Do Contract_Update

   Alter-Printer
   point-size=20

                Do Select_Contract

   Alter-Printer
   point-size=12

                BREAK
        WHEN = '006'
                PRINT 'Åõ'                      (#SE706,#LI7)
                BREAK
        WHEN = '007'
                PRINT 'Åõ'                      (#SE707,#LI7)
                BREAK
        WHEN = '008'
                PRINT 'Åõ'                      (#SE708,#LI7)
                BREAK
        WHEN = '009'
                PRINT 'Åõ'                      (#SE709,#LI7)
                BREAK
        WHEN = '010'
                PRINT 'Åõ'                      (#SE710,#LI7)
                BREAK
        WHEN = '011'
                PRINT 'Åõ'                      (#SE711,#LI7)
                Do Print_Reason (#SE711,#LI7RU,25,&HE.GPJP_UC_REASON)
                BREAK
        WHEN = '012'
                PRINT 'Åõ'                      (#SE712,#LI7)
                BREAK
        WHEN = '013'
                PRINT 'Åõ'                      (#SE713,#LI7)
                BREAK
        WHEN = '014'
                PRINT 'Åõ'                      (#SE714,#LI7)
                BREAK
        WHEN = '015'
                PRINT 'Åõ'                      (#SE715,#LI7)

   Alter-Printer
   point-size=9

                Do Select_Training

   Alter-Printer
   point-size=12

                BREAK
        WHEN = '016'
                PRINT 'Åõ'                      (#SE716,#LI7)

   Alter-Printer
   point-size=9

                PRINT &HE.GPJP_UC_OLD_LOC       (#SE716,#LI7ON)
                PRINT &HE.GPJP_UC_NEW_LOC       (#SE716,+4)

   Alter-Printer
   point-size=12

                BREAK
        WHEN = '017'
                PRINT 'Åõ'                      (#SE717,#LI7)
                Do Print_Reason (#SE717,#LI7RU,25,&HE.GPJP_UC_REASON)
                BREAK
        WHEN = '018'
                PRINT 'Åõ'                      (#SE718,#LI7)
                BREAK
        WHEN = '019'
                PRINT 'Åõ'                      (#SE719,#LI7)
                Do Print_Reason (#SE719R,#LI7R2,30,&HE.GPJP_UC_REASON)
                BREAK

! Continuous Employment After Mandatory Termination

  !Option 'I Hope'

        WHEN = '020'
                Graphic (#SE7IHOPE,#L20,16) Box 2 10
                do Select_Option
                BREAK

  !Option 'I do not Hope'

        WHEN = '021'
