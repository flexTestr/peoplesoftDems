                BREAK

END-EVALUATE


! Termination situation statement
LET $Statement1 = SUBSTR(&HE.GPJP_UC_STATEMENT,1,48)
LET $Statement2 = SUBSTR(&HE.GPJP_UC_STATEMENT,49,48)
LET $Statement3 = SUBSTR(&HE.GPJP_UC_STATEMENT,97,48)

